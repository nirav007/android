<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true"  >
        <shape >
            <solid
                android:color="#FFFFFF"  />
            <stroke
                android:width="1dp"
                android:color="#ED9118" />
            <corners
                android:radius="10dp" />
            
            
         
        </shape>
    </item>
    <item>
        <shape>
            <gradient
               android:startColor="#ED9118"
                android:endColor="#ED9118"
                android:angle="360" />
            
              <corners
                android:radius="10dp" />
            
         
        </shape>
    </item>

</selector>