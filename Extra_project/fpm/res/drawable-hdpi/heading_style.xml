<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#41A317" />
            <stroke
                android:width="1dp"
                android:color="#2f6699" />
            <corners
                android:radius="3dp" />
         
        </shape>
    </item>
    <item>
        <shape>
            <gradient
               android:startColor="#ED9118"
                android:endColor="#F5E4B1"
                android:angle="360" />
            
            <corners
                android:radius="20dp"  />
            
             <stroke
                android:width="3dp"
                android:color="#000000" />
         
        </shape>
    </item>

</selector>