<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#ffffff" />
                <size android:width="20dip"/>
            <stroke
                android:width="2dp"
                android:color="#000000" />
            <corners
                android:radius="1dp" />
          
        </shape>
    </item>
    <item>
        <shape>
            <gradient
               android:startColor="#ffffff"
                android:endColor="#ffffff"
                android:angle="360" />
           <stroke
                android:width="20dp"
                android:color="@color/tabcolor"></stroke>
            
        </shape>
    </item>
    <item  >
        <shape >
            <gradient
                 android:startColor="#ffffff"
                android:endColor="#ffffff"
                android:angle="360" />
             <stroke
                android:width="10dp"
                android:color="@color/tabcolor"></stroke>
          
        </shape>
    </item>
</selector>